THIS PART GOES IN FILE... TShirt.java
public class TShirt extends Clothing
{
    private String fabric;
    public TShirt(String aSize, String aColor, String aFabric)
    {
        super(aSize, aColor);
        fabric = aFabric;
    }
    public String getFabric()
    {
        return fabric;
    }
}
